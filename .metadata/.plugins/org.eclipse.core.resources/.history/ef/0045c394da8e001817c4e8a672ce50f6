package com.leetcode.test;

public class test35 {
	public static void main(String[] args) {
		int[] nums = new int[] { 1, 3, 5, 7 };
	}

	public int searchInsert(int[] nums, int target) {
		int index = 0;
		int[] newnums = nums;
		boolean isInput = true;
		for (int i : nums) {
			if (isInput && target < i) {
				newnums[index] = target;
				newnums[index + 1] = i;
			} else {
				newnums[index] = i;
			}
			index++;
		}
		return 0;
	}
}
